geom_bar(stat="identity")
# Horizontal bar plot
ggplot(dataset,aes(x=dose,y=len))+
geom_bar(stat="identity")+coord_flip()
# Changing the width of bars
ggplot(dataset,aes(x=dose,y=len))+
geom_bar(stat="identity" width =0.5)
# Changing the width of bars
ggplot(dataset,aes(x=dose,y=len))+
geom_bar(stat="identity", width =0.5)
# Changing color
